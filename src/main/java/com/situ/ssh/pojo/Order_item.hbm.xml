<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.situ.ssh.pojo" auto-import="true">
		<class name="Order_item" table="order_item">
			<id name="id" column="id">
				<generator class="native"></generator>
			</id>
			<property name="order_no" column="order_no" type="java.lang.String"></property>
			<property name="name" column="name" type="java.lang.String"></property>
			<property name="price" column="price" type="java.lang.Double"></property>
			<property name="amount" column="amount" type="java.lang.Integer"></property>
		</class>
</hibernate-mapping>