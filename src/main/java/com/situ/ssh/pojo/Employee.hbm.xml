<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.situ.ssh.pojo" auto-import="true">
		<class name="Employee" table="employee">
			<id name="id" column="id">
				<generator class="native"></generator>
			</id>
			<property name="name" column="name" type="java.lang.String"></property>
			<property name="role" column="role" type="java.lang.String"></property>
			<property name="phone" column="phone" type="java.lang.String"></property>
			<property name="weixin" column="weixin" type="java.lang.String"></property>
			<property name="qq" column="qq" type="java.lang.String"></property>
			<property name="time" column="time" type="java.util.Date"></property>
		</class>
</hibernate-mapping>